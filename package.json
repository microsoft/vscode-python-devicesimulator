{
  "name": "__EXTENSIONNAME__",
  "displayName": "__DISPLAYNAME__",
  "description": "__DESCRIPTION__",
  "version": "0.0.1",
  "publisher": "__PUBLISHER__",
  "engines": {
    "vscode": "^1.34.0"
  },
  "categories": [
    "Other"
  ],
  "preview": true,
  "homepage": "https://github.com/microsoft/vscode-python-embedded/blob/master/README.md",
  "bugs": {
    "url": "https://github.com/microsoft/vscode-python-embedded/issues"
  },
  "license": "SEE LICENSE IN LICENSE.md",
  "repository": {
    "type": "git",
    "url": "https://github.com/microsoft/vscode-python-embedded",
    "vsce": "^1.57.1"
  },
  "activationEvents": [
    "onCommand:adafruit.openSimulator",
    "onCommand:adafruit.runSimulator"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "adafruit.openSimulator",
        "title": "Open Simulator",
        "category": "Adafruit"
      },
      {
        "command": "adafruit.runSimulator",
        "title": "Run the Simulator",
        "category": "Adafruit"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "npm-run-all compile:*",
    "compile:extension": "tsc",
    "compile:views": "webpack --mode development",
    "compile:python": "xcopy src\\setup.py out\\ /I /Y && xcopy src\\adafruit_circuitplayground out\\adafruit_circuitplayground /S /I /E /Y",
    "watch": "npm-run-all -p watch:*",
    "watch:extension": "tsc --watch",
    "watch:views": "webpack --watch --mode development",
    "watch:python": "xcopy src\\setup.py out\\ /I /Y && xcopy src\\adafruit_circuitplayground out\\adafruit_circuitplayground /S /I /E /Y",
    "postinstall": "node ./node_modules/vscode/bin/install",
    "test": "npm run compile && node ./node_modules/vscode/bin/test",
    "lint": "tslint -c tslint.json src/**/*.{ts,tsx}",
    "format": "prettier --write src/**/*.{ts,tsx}",
    "tslint-check": "tslint-config-prettier-check ./tslint.json",
    "package": "vsce package"
  },
  "devDependencies": {
    "typescript": "^3.3.1",
    "vscode": "^1.1.28",
    "tslint": "^5.12.1",
    "@types/node": "^10.12.21",
    "@types/mocha": "^2.2.42",
    "@types/react": "16.8.18",
    "@types/react-dom": "16.8.4",
    "css-loader": "^1.0.0",
    "less": "^3.7.0",
    "less-loader": "^4.1.0",
    "npm-run-all": "^4.1.3",
    "react-scripts": "3.0.1",
    "style-loader": "^0.21.0",
    "ts-import-plugin": "^1.5.4",
    "ts-loader": "^4.4.2",
    "tslint-config-prettier": "^1.18.0",
    "tslint-microsoft-contrib": "^6.1.0",
    "tslint-react-hooks": "^2.0.0",
    "tslint-react": "^3.6.0",
    "vsce": "^1.47.0",
    "webpack": "^4.15.1",
    "webpack-cli": "^3.0.8"
  },
  "dependencies": {
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "svg-inline-react": "^3.1.0"
  },
  "eslintConfig": {
    "extends": "react-app"
  }
}
