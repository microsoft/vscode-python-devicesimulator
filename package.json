    "ts-loader": "^4.4.2",
    "tslint-config-prettier": "^1.18.0",
    "tslint-microsoft-contrib": "^6.1.0",
    "tslint-react-hooks": "^2.0.0",
    "tslint-react": "^3.6.0",
    "vsce": "^1.47.0",
    "webpack": "^4.15.1",
    "webpack-cli": "^3.0.8",
    "vscode-nls-dev": "^3.2.6"
  },
  "dependencies": {
    "@types/open": "^6.1.0",
    "open": "^6.4.0",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "svg-inline-react": "^3.1.0",
    "vscode-extension-telemetry": "^0.1.1",
    "vscode-nls": "^4.1.0"
  },
  "eslintConfig": {
    "extends": "react-app"
  }
